<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D7D7D7"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="616dp"
        android:layout_height="285dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/untitled_design__8__removebg_preview"
        tools:layout_editor_absoluteY="-9dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button"
        android:layout_width="232dp"
        android:layout_height="54dp"
        android:rotationX="0"
        android:text="Add Task"
        android:translationX="10dp"
        android:translationY="800dp"
        app:iconTint="#3F51B5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="625dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/group_9"
        tools:layout_editor_absoluteY="67dp"
        android:translationX="5dp"
        android:translationY="100dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:translationX="5dp"
        android:translationY="180dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/group_10"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="89dp" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:translationX="5dp"
        android:translationY="420dp"
        app:srcCompat="@drawable/group_13"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="396dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:translationX="5dp"
        android:translationY="550dp"
        app:srcCompat="@drawable/group_15"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="505dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:translationX="-45dp"
        android:translationY="680dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/group_14"
        tools:layout_editor_absoluteY="609dp"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>
